<script>
    const img = ["vsl01.webp", "vsl02.webp", "amb01.webp", "amb02.webp"];
    let index=0
    document.getElementById("vehicules-slide").addEventListener('click', Slide);

    function Slide() {
        index = (index + 1) % img.length;
        document.getElementById("vehicules-slide").src = "/images/" + img[index];
    }
</script>
